{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#007ACC",
          "foreground": "#222222",
          "properties": {
            "style": "austin",
            "threshold": 321
          },
          "style": "plain",
          "template": " {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "background": "#880000",
          "foreground": "#ff8888",
          "style": "plain",
          "template": "{{ if gt .Code 0 }} {{ .Code }} {{ end }}",
          "type": "status"
        },
        {
          "background": "#ffcc88",
          "foreground": "#222222",
          "style": "plain",
          "template": "\uf0e7",
          "type": "root"
        },
        {
          "background": "#222222",
          "foreground": "#666666",
          "style": "plain",
          "template": " root ",
          "type": "root"
        },
        // {
        //   "background": "#222222",
        //   "foreground": "#666666",
        //   "style": "plain",
        //   "template": " {{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }}@{{ .HostName }} ",
        //   "type": "session"
        // },
        {
          "background": "#444444",
          "foreground": "#cccccc",
          "properties": {
            "folder_icon": "\u2026",
            "style": "mixed",
            "mapped_locations_enabled": true,
            "mapped_locations": {
              "~/Developer": "~dev",
              "~/hdd": "~hdd",
              "~/HDD": "~hdd"
            }
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#cf432B",
          "foreground": "#f1f0e9",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "style": "plain",
          "template": " {{ .HEAD }} ",
          "type": "git"
        },
        {
          "background": "#83BA63",
          "foreground": "#ffffff",
          "properties": {
            "display_mode": "files",
            "cache_version": true
          },
          "style": "plain",
          "template": " {{ if .Expected }}{{ .Expected }}{{else}}{{ .Full }}{{end}} ",
          "type": "node"
        },
        {
          "background": "#7FD5EA",
          "foreground": "#ffffff",
          "properties": {
            "fetch_version": false
          },
          "style": "plain",
          "template": " \ue626 ",
          "type": "go"
        },
        {
          "background": "#ffd153",
          "foreground": "#3a3a3a",
          "style": "plain",
          "template": "  {{ .Major }}.{{ .Minor }} ",
          "properties": {
            "fetch_virtual_env": true,
            "fetch_version": true,
            "display_mode": "files"
          },
          "type": "python"
        },
        {
          "background": "#99908a",
          "foreground": "#193549",
          "properties": {
            "fetch_version": false
          },
          "style": "plain",
          "template": " \ue7a8 ",
          "type": "rust"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#ffa600",
          "style": "plain",
          "template": "❯ ",
          "type": "shell",
          "properties": {
            "mapped_shell_names": {
              "pwsh": "\u276f",
              "shell": "\u276f",
              "cmd": ">",
              "lua": ">",
              "nu": ":)",
              "fish": "~>",
              "zsh": "%",
              "bash": "$"
            }
          }
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}